@page
@model WarehouseMaster.Pages.TransactionList.IndexModel

@{ ViewData["Title"] = "Transaction List"; }

<br />
<div class="container row p-0 m-0">
    <div class="col-9">
        <h2 class="text-info">Transactions </h2>
    </div>
    <div class="col-3">
        <a asp-page="create" class="btn btn-info form-control text-white">Record Transaction</a>
    </div>
    <br />
    
    <div class="align-content-lg-stretch border p-3">

        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
   
                <tr>
                    <th>Transaction Date</th>
                    <th>Transaction Name</th>
                    <th>Transaction Description</th>
                    <th>Transaction Invoice/Ref#</th>
                    <th>Product Id</th>
                    <th>Product Name</th>
                    <th>Inventory IN (Unit)</th>
                    <th>Inventory OUT (Unit)</th>
                    <th>Inventory Balance (Unit)</th>
                    <th>Product Cost PU</th>
                    <th>Product Selling Price PU</th>
                    <th>Inventory Total Cost</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts{
    <script src="~/js/TransactionList.js"></script>
}





