@page
@model WarehouseMaster.Pages.ProductList.IndexModel

@{ ViewData["Title"] = "Product List"; }

<br />
<div class="container row p-0 m-0">
    <div class="col-9">
        <h2 class="text-info">Product List</h2>
    </div>
    <div class="col-3">
        <a asp-page="create" class="btn btn-info form-control text-white">Add New Product</a>
    </div>
</div>


<div>
    <div class="col-12 border p-3">

        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Product Name</th>
                    <th>Product Desc.</th>
                    <th>Vendor Id</th>
                    <th>Vendor Name</th>
                    <th>Cost Price</th>
                    <th>Margin(%)</th>
                    <th>SellingPrice</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts{
    <script src="~/js/ProductList.js"></script>
}



