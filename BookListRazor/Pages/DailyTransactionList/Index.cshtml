@page
@model WarehouseMaster.Pages.DailyTransactionList.IndexModel

@{ ViewData["Title"] = "Transactions List"; }

<br />
<div class="container row p-0 m-0">
    <div class="col-9">
        <h2 class="text-info">Daily Transactions</h2>
    </div>
    <div class="col-3">
        <a asp-page="create" class="btn btn-info form-control text-white">Record New Transaction</a>
    </div>
     
    <div class="col-12 border p-3">

        <table id="DT_loadTran" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Transaction Date</th>
                    <th>Transaction Name</th>
                    <th>Transaction Details</th>
                    <th>Transaction in Amount</th>
                    <th>Transaction Out Amt</th>
                    <th>Remaining Balance</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts{
    <script src="~/js/DailyTransactionList.js"></script>
}





