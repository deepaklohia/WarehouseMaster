@page
@model WarehouseMaster.Pages.VendorList.IndexModel

@{ ViewData["Title"] = "Vendor List"; }

<br />
<div class="container row p-0 m-0">
    <div class="col-9">
        <h2 class="text-info">Vendor List</h2>
    </div>
    <div class="col-3">
        <a asp-page="create" class="btn btn-info form-control text-white">Add New Vendor</a>
    </div>
</div>

<div>
    <div class="col-12 border p-3">

        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Vendor Name</th>
                    <th>Vendor Address</th>
                    <th>Vendor Phone</th>
                    <th>Vendor Email</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts{
    <script src="~/js/VendorList.js"></script>
}





